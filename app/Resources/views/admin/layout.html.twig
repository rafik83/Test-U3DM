    <div class="main-content container">
        {% for label, messages in app.flashes %}
            {% if 'success' == label %}
                {% set icon = 's7-check' %}
            {% elseif 'warning' == label %}
                {% set icon = 's7-attention' %}
            {% elseif 'danger' == label %}
                {% set icon = 's7-close' %}
            {% else %}
                {% set icon = 's7-info' %}
            {% endif %}
            {% for message in messages %}
                <div role="alert" class="alert alert-{{ label }} alert-dismissible">
                    <button type="button" data-dismiss="alert" aria-label="{{ 'flash.close'|trans }}" class="close"><span aria-hidden="true" class="s7-close"></span></button>
                    <div class="icon"><span class="{{ icon }}"></span></div>
                    <div class="message">{{ message|trans }}</div>
                </div>
            {% endfor %}
        {% endfor %}
